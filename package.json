{
    "name": "openrouter",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "check-types": "turbo run check-types",
        "clean": "rm -rf node_modules **/*/node_modules **/*/.turbo **/*/dist .turbo dist"
    },
    "devDependencies": {
        "prettier": "^3.7.4",
        "turbo": "^2.8.9",
        "typescript": "5.9.2"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "bun@1.3.6",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
